{
  "expo": {
    "name": "YummyYummix",
    "slug": "yummy-yummix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "yummyyummix",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "com.yummyyummix.app",
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Allow YummyYummix to access your microphone for voice conversations with Irmixy."
      },
      "associatedDomains": [
        "applinks:app.yummyyummix.com"
      ]
    },
    "android": {
      "package": "com.yummyyummix.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app/adaptive-icon.png",
        "backgroundColor": "#FCF6F2"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "app.yummyyummix.com",
              "pathPrefix": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app/favicon.png",
      "router": {
        "mode": "path"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://app.yummyyummix.com"
        }
      ],
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app/splash.png",
          "resizeMode": "cover"
        }
      ],
      "expo-localization",
      "expo-secure-store",
      "expo-apple-authentication",
      "expo-web-browser",
      "expo-audio",
      "@config-plugins/react-native-webrtc",
      "expo-navigation-bar",
      "expo-speech-recognition"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1717213e-1de6-40b1-94f0-6c2b22889729"
      }
    },
    "owner": "banana-boogie"
  }
}