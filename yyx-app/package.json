  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "ios:local": "expo run:ios --device",
    "ios:prod": "mv .env.local .env.local.backup 2>/dev/null || true && expo run:ios --device && mv .env.local.backup .env.local 2>/dev/null || true",
    "web": "expo start --web",
    "test": "jest --watch --no-watchman",
    "test:ci": "jest --watchAll=false --coverage --passWithNoTests --no-watchman",
    "test:coverage": "jest --coverage --watchAll=false --no-watchman",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "expo lint",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
